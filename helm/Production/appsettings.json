{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithCorrelationIdHeader",
      "WithThreadId",
      "WithClientAgent",
      "WithExceptionDetails"
    ],
    "Properties": {
      "Application": "portal-publico"
    }
  },
  "AmazonS3": {
    "BucketName": "agreement-info-mirae-prd"
  },
  "AllowedHosts": "*",
  "Swagger": {
    "Prefix": "/",
    "Title": "Portal-Publico",
    "Version": "V1",
    "XmlFile": "portal-publico"
  }
}